@using TravelTripProje.Models.Sınıflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>HAKKIMIZDA</h3>

            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" width="1000" height="370"/>
                <p>@x.Aciklama</p>
            }
        </div>
    </div>
</div>



