@using TravelTripProje.Models.Sınıflar
@model List<Blog> 
<div class="padding1">
    <br />
    <h4>EN BEĞENİLEN YERLER</h4>
    <div class="box-left">
        @foreach(var x in Model)
        {
        <div class="bot-img">
            <img src="@x.BlogImage" style="width:120px; height:80px;" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.Aciklama.Substring(0,80)... </p>
        </div>
        <div class="clear"></div>
        }
    </div>
        @Html.Action("Partial5","Default")
</div>
